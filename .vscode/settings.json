{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/*.o": true,
		"**/*.exe": true,
	},
	"makefile.configureOnOpen": false,
	"files.associations": {
		"*.h": "c",
		"*.bison": "yacc",
		"*.cgen": "c",
		"*.m": "c",
		"typeinfo": "c",
		"array": "c",
		"string_view": "c",
		"initializer_list": "c"
	}
}